        {
          "alias": "formbuilder",
          "_isSecondary": true,
          "_styles": {
            "padding-top": "(@paddingTop * 15px)",
            "padding-bottom": "(@paddingBottom * 15px)",
            ".mbr-overlay": {
              "background-color": "@overlayColor",
              "opacity": "@overlayOpacity"
            },
            "& when (@bg-type = 'image')": {
              "background-image": "url(@bg-value)"
            },
            "& when (@bg-type = 'color')": {
              "background-color": "@bg-value"
            },
            "& when (@customStyle)": {
              ".form-control, .field-input": {
                "padding": "0.5rem",
                "background-color": "@fieldColor",
                "border-color": "@borderColor",
                "color": "@formText",
                "transition": "0.4s",
                "box-shadow": "none",
                "outline": "none",
                "input::-webkit-input-placeholder, textarea::-webkit-input-placeholder": {
                  "color": "@formPlaceholder"
                },
                "input:-moz-placeholder, textarea:-moz-placeholder": {
                  "color": "@formPlaceholder"
                },
                "&:hover, &:focus": {
                  "background-color": "@fieldHover",
                  "border-color": "@borderHover",
                  "color": "contrast(@formText, #000000, #ffffff, 30%)",
                  "box-shadow": "none",
                  "outline": "none",
                  "input::-webkit-input-placeholder, textarea::-webkit-input-placeholder": {
                    "color": "contrast(@formPlaceholder, #000000, #ffffff, 30%)"
                  },
                  "input:-moz-placeholder, textarea:-moz-placeholder": {
                    "color": "contrast(@formPlaceholder, #000000, #ffffff, 30%)"
                  }
                }
              },
              ".jq-number__spin:hover, .jq-number__spin:focus": {
                "background-color": "@fieldHover",
                "border-color": "@borderHover",
                "color": "contrast(@formText, #000000, #ffffff, 30%)",
                "box-shadow": "none",
                "outline": "none"
              },
              ".jq-number__spin": {
                "background-color": "@fieldColor",
                "border-color": "@borderColor",
                "color": "@formText",
                "transition": "0.4s",
                "box-shadow": "none",
                "outline": "none"
              },
              ".jq-selectbox li, .jq-selectbox li": {
                "background-color": "@fieldColor",
                "color": "contrast(@fieldColor, #000000, #ffffff, 30%)"
              },
              ".jq-selectbox li:hover, .jq-selectbox li.selected": {
                "background-color": "@fieldHover",
                "color": "contrast(@fieldHover, #000000, #ffffff, 30%)"
              },
              ".jq-selectbox:hover .jq-selectbox__trigger-arrow, .jq-number__spin.minus:hover:after, .jq-number__spin.plus:hover:after": {
                "border-top-color": "@fieldColor",
                "border-bottom-color": "@fieldColor"
              },
              ".jq-selectbox .jq-selectbox__trigger-arrow, .jq-number__spin.minus:after, .jq-number__spin.plus:after": {
                "border-top-color": "@fieldHover",
                "border-bottom-color": "@fieldHover"
              }
            },
            "& when (@customStyle), (@disableElements)": {
              "input::-webkit-clear-button": {
                "display": "none"
              },
              "input::-webkit-inner-spin-button": {
                "display": "none"
              },
              "input::-webkit-outer-spin-button": {
                "display": "none"
              },
              "input::-webkit-calendar-picker-indicator": {
                "display": "none"
              }
            }
          },
          "_name": "formbuilder",
          "_customHTML": "<section class=\"form\" group=\"Form\" plugins=\"formstyler, datepicker\">\n    <mbr-parameters>\n        <header>Size</header>\n        <input type=\"range\" inline=\"\" title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"0\">\n        <input type=\"range\" inline=\"\" title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"2\">\n        <header>Style</header>\n        <input type=\"checkbox\" name=\"customStyle\" title=\"Custom Style\">\n        <input type=\"checkbox\" name=\"disableElements\" title=\"Disable Control\">\n        <input type=\"color\" name=\"fieldColor\" title=\"Field\" value=\"#ffffff\" condition=\"customStyle\">\n        <input type=\"color\" name=\"fieldHover\" title=\"Field Hover\" value=\"#efefef\" condition=\"customStyle\">\n        <input type=\"color\" name=\"borderColor\" title=\"Border\" value=\"#cccccc\" condition=\"customStyle\">\n        <input type=\"color\" name=\"borderHover\" title=\"Border Hover\" value=\"#000000\" condition=\"customStyle\">\n        <input type=\"color\" name=\"formText\" title=\"Text\" value=\"#000000\" condition=\"customStyle\">\n        <input type=\"color\" name=\"formPlaceholder\" title=\"Placeholders\" value=\"#c1c1c1\" condition=\"customStyle\">\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\">\n            <input type=\"image\" title=\"Background Image\" value=\"../assets/background.jpg\">\n            <input type=\"color\" title=\"Background Color\" value=\"#ffffff\" selected=\"\">\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked=\"\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#ffffff\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n        <input type=\"range\" inline=\"\" title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.4\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n    </mbr-parameters>\n    <div class=\"mbr-overlay\" mbr-if=\"(overlay &amp;&amp; bg.type=='image')\"></div>\n    <div class=\"container\">\n        <div mbr-form=\"\" class=\"col-lg-8 mx-auto mbr-form\">\n<!--Formbuilder Form-->\n<form action=\"hawaiimeditationretreats@gmail.com\" name=\"3-day July Retreat Registration\" method=\"POST\" class=\"mbr-form form-with-styler\">\n<div class=\"dragArea form-row\">\n<div class=\"col-lg-12\">\n<h4 mbr-theme-style=\"display-5\" class=\"mbr-fonts-style\">\n</h4>\n</div>\n<div class=\"col-lg-12\">\n<hr>\n</div>\n<div data-for=\"Name\" class=\"col-lg-12 form-group\">\n<label mbr-text=\"\" mbr-theme-style=\"display-7\" for=\"Name\" class=\"form-control-label mbr-fonts-style\">Name (required)</label>\n<input type=\"text\" name=\"Name\" placeholder=\"Name\" data-form-field=\"Name\" mbr-theme-style=\"display-7\" class=\"form-control\" required=\"required\" value=\"\">\n</div>\n<div data-for=\"Email\" class=\"col-lg-12 form-group\">\n<label mbr-text=\"\" mbr-theme-style=\"display-7\" for=\"Email\" class=\"form-control-label mbr-fonts-style\">Email (required)</label>\n<input type=\"email\" name=\"Email\" placeholder=\"Email\" data-form-field=\"Email\" mbr-theme-style=\"display-7\" class=\"form-control\" required=\"required\" value=\"\">\n</div>\n<div data-for=\"Address\" class=\"col-lg-12 form-group\">\n<label mbr-text=\"\" mbr-theme-style=\"display-7\" for=\"Address\" class=\"form-control-label mbr-fonts-style\">Address (required)</label>\n<textarea name=\"Address\" data-form-field=\"Address\" mbr-theme-style=\"display-7\" required=\"required\" class=\"form-control\" value=\"\"></textarea>\n</div>\n<div data-for=\"Phone\" class=\"col-lg-12 form-group\">\n<label mbr-text=\"\" mbr-theme-style=\"display-7\" for=\"Phone\" class=\"form-control-label mbr-fonts-style\">Phone (required)</label>\n<input type=\"tel\" name=\"Phone\" placeholder=\"Phone\" pattern=\"\\+?[0-9]+\" data-form-field=\"Phone\" mbr-theme-style=\"display-7\" required=\"required\" class=\"form-control\" value=\"\">\n</div>\n<div class=\"col-lg-12 form-group\" data-for=\"Gender\">\n<div class=\"form-control-label\">\n<label mbr-text=\"\" mbr-theme-style=\"display-7\" for=\"Gender\" class=\"mbr-fonts-style\">Gender (for room assignments)</label>\n</div>\n<div class=\"form-check\">\n<input type=\"radio\" name=\"Gender\" data-form-field=\"Gender\" mbr-theme-style=\"display-7\" class=\"form-check-input\" value=\"Female\" checked=\"checked\">\n<label mbr-text=\"\" mbr-theme-style=\"display-7\" class=\"form-check-label\">Female</label>\n</div>\n<div class=\"form-check\">\n<input type=\"radio\" name=\"Gender\" data-form-field=\"Gender\" mbr-theme-style=\"display-7\" class=\"form-check-input\" value=\"Male\">\n<label mbr-text=\"\" mbr-theme-style=\"display-7\" class=\"form-check-label\">Male</label>\n</div>\n<div class=\"form-check\">\n<input type=\"radio\" name=\"Gender\" data-form-field=\"Gender\" mbr-theme-style=\"display-7\" class=\"form-check-input\" value=\"Other\">\n<label mbr-text=\"\" mbr-theme-style=\"display-7\" class=\"form-check-label\">Other/Prefer not to say</label>\n</div>\n</div>\n<div data-for=\"Source\" class=\"col-lg-12 form-group\" style=\"\">\n<label mbr-text=\"\" mbr-theme-style=\"display-7\" for=\"Source\" class=\"form-control-label mbr-fonts-style\">How did hear about us? (required)</label>\n<select name=\"Source\" data-form-field=\"Source\" mbr-theme-style=\"display-7\" class=\"form-control\">\n<option value=\"Select\">Select</option>\n<option value=\"FaceBook/IG\">FaceBook/IG</option>\n<option value=\"Web Search\">Web Search</option>\n<option value=\"Friend\">Friend</option>\n<option value=\"Other\">Other</option>\n</select>\n</div>\n<div data-for=\"Prior Retreats\" class=\"col-lg-12 form-group\">\n<label mbr-text=\"\" mbr-theme-style=\"display-7\" for=\"Prior Retreats\" class=\"form-control-label mbr-fonts-style\">Have you participated in a silent retreat before? Please note any experiences.</label>\n<textarea name=\"Prior Retreats\" data-form-field=\"Prior Retreats\" mbr-theme-style=\"display-7\" class=\"form-control\" value=\"\"></textarea>\n</div>\n<div data-for=\"Meditation Practice\" class=\"col-lg-12 form-group\" style=\"\">\n<label mbr-text=\"\" mbr-theme-style=\"display-7\" for=\"Meditation Practice\" class=\"form-control-label mbr-fonts-style\">Do you have a meditation practice? If so, how long practicing? Please note any specific styles/traditions and experiences.</label>\n<textarea name=\"Meditation Practice\" data-form-field=\"Meditation Practice\" mbr-theme-style=\"display-7\" class=\"form-control\" value=\"\"></textarea>\n</div>\n<div data-for=\"Yoga Practice\" class=\"col-lg-12 form-group\" style=\"\">\n<label mbr-text=\"\" mbr-theme-style=\"display-7\" for=\"Yoga Practice\" class=\"form-control-label mbr-fonts-style\">Do you have a yoga practice? If so, how long practicing? Please note any specific styles/traditions and experiences.</label>\n<textarea name=\"Yoga Practice\" data-form-field=\"Yoga Practice\" mbr-theme-style=\"display-7\" class=\"form-control\" value=\"\"></textarea>\n</div>\n<div data-for=\"What Attracts You\" class=\"col-lg-12 form-group\">\n<label mbr-text=\"\" mbr-theme-style=\"display-7\" for=\"What Attracts You\" class=\"form-control-label mbr-fonts-style\">What attracts you to meditation and yoga?</label>\n<textarea name=\"What Attracts You\" data-form-field=\"What Attracts You\" mbr-theme-style=\"display-7\" class=\"form-control\" value=\"\"></textarea>\n</div>\n<div data-for=\"Physical Limits\" class=\"col-lg-12 form-group\" style=\"\">\n<label mbr-text=\"\" mbr-theme-style=\"display-7\" for=\"Physical Limits\" class=\"form-control-label mbr-fonts-style\">Do you have any injuries or physical limitations we should be aware of to support your yoga practice? </label>\n<textarea name=\"Physical Limits\" data-form-field=\"Physical Limits\" mbr-theme-style=\"display-7\" class=\"form-control\" value=\"\"></textarea>\n</div>\n<div data-for=\"Diet\" class=\"col-lg-12 form-group\">\n<label mbr-text=\"\" mbr-theme-style=\"display-7\" for=\"Diet\" class=\"form-control-label mbr-fonts-style\">Your primary diet (vegetarian, vegan, other)</label>\n<input type=\"text\" name=\"Diet\" data-form-field=\"Diet\" mbr-theme-style=\"display-7\" class=\"form-control\" value=\"\">\n</div>\n<div data-for=\"Food Allergies\" class=\"col-lg-12 form-group\" style=\"\">\n<label mbr-text=\"\" mbr-theme-style=\"display-7\" for=\"Food Allergies\" class=\"form-control-label mbr-fonts-style\">Any food allergies we should be aware of?</label>\n<input type=\"text\" name=\"Food Allergies\" data-form-field=\"Food Allergies\" mbr-theme-style=\"display-7\" class=\"form-control\" value=\"\">\n</div>\n<div data-for=\"Mobility Challenges\" class=\"col-lg-12 form-group\">\n<label mbr-text=\"\" mbr-theme-style=\"display-7\" for=\"Mobility Challenges\" class=\"form-control-label mbr-fonts-style\">What mobility challenges or disabilities do you currently experience? Are you able to comfortably use stairs? This info will help us with a room assignment that best serves your body. </label>\n<textarea name=\"Mobility Challenges\" placeholder=\"\" data-form-field=\"Mobility Challenges\" mbr-theme-style=\"display-7\" class=\"form-control\" value=\"\"></textarea>\n</div>\n<div data-for=\"Emergency Contact\" class=\"col-lg-12 form-group\" style=\"\">\n<label mbr-text=\"\" mbr-theme-style=\"display-7\" for=\"Emergency Contact\" class=\"form-control-label mbr-fonts-style\">Emergency Contact (required)</label>\n<textarea name=\"Emergency Contact\" placeholder=\"Name, Relationship, Email, Phone number\" data-form-field=\"Emergency Contact\" mbr-theme-style=\"display-7\" class=\"form-control\" required=\"required\" value=\"\"></textarea>\n</div>\n<div data-for=\"Roommate\" class=\"col-lg-12 form-group\">\n<label mbr-text=\"\" mbr-theme-style=\"display-7\" for=\"Roommate\" class=\"form-control-label mbr-